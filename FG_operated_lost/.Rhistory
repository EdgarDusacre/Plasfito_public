# Fonction pour calculer les statistiques d'intérêt (inchangée)
calc_stats <- function(data, indices) {
d <- data[indices,]
predictions <- predict(model_gam_ig, newdata = d, type = "response", se.fit = TRUE)
d <- d %>%
mutate(
Surface_net_log = predictions$fit,
Surface_net_m2 = 10^Surface_net_log,
lost_surface_net = Surface_net_m2 * 0.0081
) %>%
summarise(
sum_operated = sum(Surface_net_m2),
sum_lost = sum(lost_surface_net)
)
return(c(d$sum_operated, d$sum_lost))
}
# Fonction pour effectuer le bootstrap et calculer les résultats pour un groupe
bootstrap_group <- function(data) {
set.seed(123)  # Pour la reproductibilité
boot_results <- boot(data = data, statistic = calc_stats, R = 5000)
medians <- apply(boot_results$t, 2, median)
ci_operated <- boot.ci(boot_results, type = "perc", index = 1)
ci_lost <- boot.ci(boot_results, type = "perc", index = 2)
data.frame(
Variable = c("sum_operated", "sum_lost"),
Median = medians,
CI_Lower = c(ci_operated$percent[4], ci_lost$percent[4]),
CI_Upper = c(ci_operated$percent[5], ci_lost$percent[5])
)
}
